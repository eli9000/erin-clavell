{
  "name": "erin-clavell",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p start:server start:client watch:css",
    "start:client": "react-scripts start",
    "start:server": "nodemon server --watch server --exec babel-node",
    "build": "npm run build:css && react-scripts build",
    "build:css": "node-sass-chokidar src -o src/styles",
    "watch:css": "npm run build:css && node-sass-chokidar src -o src/styles --watch --recursive"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "node-sass-chokidar": "^0.0.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2"
  }
}
